{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "singularName": "player",
    "pluralName": "players",
    "displayName": "Player",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Age": {
      "type": "integer"
    },
    "Height": {
      "type": "integer"
    },
    "Weight": {
      "type": "decimal"
    },
    "Photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Birthday": {
      "type": "date"
    },
    "gender": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gender.gender"
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "mappedBy": "players"
    },
    "leagues": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::league.league",
      "inversedBy": "players"
    },
    "Surname": {
      "type": "string"
    },
    "positions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::position.position",
      "inversedBy": "players"
    },
    "Dorsal": {
      "type": "integer"
    }
  }
}
